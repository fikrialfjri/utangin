{
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^react",
    "^react/(.*)$",
    "",
    "^vite",
    "^vite/(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/components/(.*)$",
    "",
    "^@/hooks/(.*)$",
    "",
    "^@/libs/(.*)$",
    "",
    "^@/utils/(.*)$",
    "",
    "^@/assets/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "pluginSearchDirs": false
}
